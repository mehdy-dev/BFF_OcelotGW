{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost"
  },
  "AllowedHosts": "*",
  "OidcProxy": {
    "LandingPage": "/.auth/login",
    "EnableUserPreferredLandingPages": true,
    "EntraId": {
        "ClientId": "you app id",
        "ClientSecret": "you app secret",
        "TenantId": "your tenant id",
        "Scopes": [
            "openid",
            "profile",
            "offline_access",
            "your api key "
        ]
    },
    "ReverseProxy": {
      "Routes": {
        "api": {
          "ClusterId": "api",
            "Match": {
                "Path": "{**catch-all}"
            }
        }
      },
      "Clusters": {
        "api": {
          "Destinations": {
            "api/node1": {
              "Address": "https://localhost:7049/"
            }
          }
        }
      }
    }
  }
}
